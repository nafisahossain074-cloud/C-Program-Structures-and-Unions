#include <stdio.h>
struct Distance {
    int feet;
    float inch;
};
int main() {
    int n;
    printf("Enter number of distances: ");
    scanf("%d", &n);
    struct Distance d[n], sum = {0, 0.0};
    for(int i = 0; i < n; i++) {
        printf("Enter distance %d (feet inch): ", i+1);
        scanf("%d %f", &d[i].feet, &d[i].inch);
    }
    for(int i = 0; i < n; i++) {
        sum.feet += d[i].feet;
        sum.inch += d[i].inch;
    }
    while(sum.inch >= 12.0) {
        sum.inch -= 12.0;
        sum.feet += 1;
    }

    printf("Total distance = %d feet %.2f inches\n", sum.feet, sum.inch);

    return 0;
}
